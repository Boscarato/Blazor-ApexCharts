@namespace ApexCharts


<ApexChart TItem="GaugeValue" Title="@Title"
           
           Options="options">

    <ApexPointSeries TItem="GaugeValue"
    SeriesType=PointType.RadialBar
                Items=GetItems()
                Name=@Label
                XValue="@(e => e.Label)"
                YValue="@(e => e.Precentage)" />
</ApexChart>



